<h1>Listing Articles</h1>
<%= link_to "create new article", new_article_path %>
<br /><br />

<table>
<tr>
  <td><strong>Title</strong></td>
  <td><strong>Description</strong></td>
</tr>
<% @articles.each do |article| %>
  <tr>
    <td><%= article.title %></td>
    <td><%= article.description %></td>
  </tr>
<% end %>
</table>